# Server Configuration
PORT=5001
NODE_ENV=development
CORS_ORIGIN=http://localhost:3000

# Database (Supabase Session pooler - рекомендуется)
DATABASE_URL="postgresql://postgres.xxxxx:your_password@aws-1-eu-west-1.pooler.supabase.com:5432/postgres?sslmode=require"

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# JWT
JWT_SECRET=your_jwt_secret_key_change_in_production
JWT_EXPIRES_IN=7d

# Telegram Bot
BOT_TOKEN=your_telegram_bot_token_here

# Cloudpub Configuration (опционально)
CLOUDPUB_BACKEND_URL=https://your-backend-url.cloudpub.ru
CLOUDPUB_FRONTEND_URL=https://your-frontend-url.cloudpub.ru
